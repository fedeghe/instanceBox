{
	"EXE" : [
		"rm -rf lib test",
		"mkdir -p lib",
		"mkdir -p test/css"
	],
	"source/lib/instanceBox.js" : "lib -plugins=malta-js-uglify",
	"source/index.html" : ". -options=showPath:false",
	"source/html/*.html" : "test",
	"source/test/*.js" : "test -vars=source/vars.json",
	"source/*.js" : ". -vars=source/vars.json",
	"source/less/*.less" : "test/css -plugins=malta-less"
}