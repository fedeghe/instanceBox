{
	"EXE" : [
		"rm -rf lib manualtest",
		"mkdir -p lib",
		"mkdir -p manualtest/css manualtest/js"
	],
	"source/lib/instanceBox.js" : [
		"lib -plugins=malta-js-uglify",
		"manualtest/js"
	],
	"source/manualtest/*.html" : "manualtest",
	"source/manualtest/*.js" : "manualtest/js -vars=source/vars.json",
	"source/manualtest/*.less": "manualtest/css -plugins=malta-less"
}